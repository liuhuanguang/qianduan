/**app.wxss**/

.bf{
  background-color: #fff;
}
.bor{
  border-radius: 5px;
}
button::after{
  border:none;
}
input{
outline:none;
border:none;
list-style: none;
}
.container {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-content:space-around /*flex-start*/;
  box-sizing: border-box;
}
.container_c{
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.flex_center{
  display: flex;
  justify-content: center;
  align-items: center;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.cf:after {
  content: "";
  display: table;
  height: 0;
  clear: both;
  visibility: hidden;
}
.p3{
  padding: 3%;
}
.ptb{
  padding:0 3%;
}
.pb{
  padding-bottom: 250rpx;
}
.omit{
  overflow:hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
}
.fix_nav{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99;
  height: 134rpx;
  background-color: #fff;
  border-top: 1px solid #ddd;
}
.fix_nav navigator{
  text-align: center;
  width: 25%;
}
.fix_nav image{
  width: 40rpx;
  height: 39rpx;
  display: block;
  margin: 0 auto 6rpx auto;
}
.fix_nav text{
  font-size: 24rpx;
  color: #bebebe;
  display: block;
}
.fix_nav .text{
  font-size: 22rpx;
}
.fix_nav navigator.on text{
  color: #60a1f3;
}
.fixed_nav{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 999;
  height: 100rpx;
  line-height: 100rpx;
}
.big_bnt{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #E04239;
  font-size: 28rpx;
  color: #fff;
  border-radius: 3px;
}
.big_bnt9{
  width: 90%;
  margin: 0 auto;
}
.bnt2{
  width: 20%;
  height: 70rpx;
  line-height: 70rpx;
} 
.fixed{
  position: fixed;
  width: 100%;
}
.service{
  width: 120rpx;
  height: 120rpx;
  position: fixed;
  bottom: 13%;
  right: 2%;
  z-index: 11;
}
.service image{
  display: block;
  width: 120rpx;
  height: 120rpx;
}
.mask{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, .5);
}