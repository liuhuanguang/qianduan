/* pages/ collect/ collect.wxss */.list{
  padding: 0 3%;
}
.list .logo{
  width:120rpx;
  height:120rpx;
  border-radius: 100%;
  margin-top: 30rpx;
}
.list .neirong{
  padding: 30rpx 0;
  border-bottom: 1px solid #ddd;
  width: 75%;
}
.list .neirong .name{
  height:60rpx;
  font-size: 30rpx;
  color: #333;
}
.list .neirong .text{
  font-size: 24rpx;
  color: #999;
}
.list .neirong .close{
  width: 40rpx;
  height: 40rpx;
  line-height: 15rpx;
  color: #ccc;
  font-size: 35rpx;
  text-align: right;
}
.list .neirong .down{
  height: 60rpx;
}
.list .neirong .down .text{
  margin-top: 24rpx;
}
.list .neirong .down navigator{
  height: 60rpx;
  width: 60rpx;
}
.list .neirong .down image{
  width: 32rpx;
  height: 8rpx;
  margin-top: 40rpx;
  margin-left: 23rpx;
}
.cart-no-data{
    padding:100rpx 0;
    color: #999;
    text-align: center;
    font-size:30rpx;
}