.my_head{
  height: 235rpx;
  width: 100%;
  position: relative;
  align-items:center;
  padding: 0 31rpx;
  box-sizing: border-box;
}
.my_head .my_back{
  width: 100%;
  height: 235rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.my_head .my_left{
  position: relative;
}
.my_head .my_left .logo{
  width: 120rpx;
  height: 120rpx;
  display: block;
  border-radius: 100%;
  margin-right: 26rpx;
}
.my_head .my_left .name{
  font-size: 40rpx;
  color: #fff;
  margin-top: 10rpx;
  text-align: center;
}
.my_head .my_left .job{
  font-size: 24rpx;
  color: #fff;
  margin-top: 17rpx;
  text-align: center;
}
.my_head .my_right{
  width: 202rpx;
  height: 60rpx;
  line-height: 60rpx;
  background-color: #fff;
  border-radius: 2px;
  position: relative;
}
.my_head .my_right image{
  width: 38rpx;
  height: 33rpx;
  margin: 14rpx 10rpx 0 27rpx;
}
.my_head .my_right view{
  font-size: 28rpx;
  color: #E04239;
}
.my_list .list{
  height: 105rpx;
  line-height: 105rpx;
  border-bottom: 1px solid #f1f1f1;
  padding: 0 4%;
}
.my_list .list .l_img{
  width: 40rpx;
  height: 40rpx;
  margin-top: 32rpx;
}
.my_list .list .l_text{
  font-size: 28rpx;
  color: #666;
  margin-left: 20rpx;
}
.my_list .list .r_text{
  font-size: 24rpx;
  color: #ccc;
  margin-right: 20rpx;
}
.my_list .list .r_img{
  width: 14rpx;
  height: 28rpx;
  margin-top: 40rpx;
}
.my_list .my_gg{
  width: 100%;
  height: 241rpx;
  padding: 0;
  position: relative;
}
.my_list .my_gg image{
  width: 100%;
  height: 241rpx;
  margin: 0;
}
.my_list .my_gg text{
  position: absolute;
  top: 11rpx;
  left: 11rpx;
  width: 66rpx;
  height: 36rpx;
  line-height: 36rpx;
  background-color: rgba(0, 0, 0, .5);
  text-align: center;
  font-size: 24rpx;
  color: #fff;
  margin-left: 0;
}
switch{
  zoom: .6!important;
}